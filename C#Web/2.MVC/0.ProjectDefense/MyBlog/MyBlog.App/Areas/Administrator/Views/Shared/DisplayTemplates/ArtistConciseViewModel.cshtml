@model ArtistConciseViewModel

<tr>
    <td>@Model.Id</td>
    <td><a asp-page="/Music/ArtistDetails" asp-route-id="@Model.Id">@Model.FullName</a></td>
    <td>
        <a asp-area="Administrator" asp-page="/Music/ArtistDetails" asp-route-id="@Model.Id"
           class="btn btn-default">Details</a>
        <a asp-area="Administrator" asp-controller="Artists" asp-action="EditArtist" asp-route-id="@Model.Id"
           class="btn btn-warning">
            Edit
        </a>
        <a asp-area="Administrator" asp-controller="Artists" asp-action="DeleteArtist" asp-route-id="@Model.Id"
           class="btn btn-danger">
            Delete
        </a>
    </td>
</tr>
