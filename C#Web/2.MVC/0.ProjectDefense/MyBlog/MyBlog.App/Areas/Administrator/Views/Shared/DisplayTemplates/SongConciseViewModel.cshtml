@model SongConciseViewModel

<tr>
    <td>@Model.Id</td>
    <td><a asp-page="/Music/SongDetails" asp-route-id="@Model.Id">@Model.SongName</a></td>
    <td>
        <a asp-area="Administrator" asp-page="/Music/SongDetails" asp-route-id="@Model.Id"
           class="btn btn-default">Details</a>
        <a asp-area="Administrator" asp-controller="Songs" asp-action="EditSong" asp-route-id="@Model.Id"
           class="btn btn-warning">
            Edit
        </a>
        <a asp-area="Administrator" asp-controller="Songs" asp-action="DeleteSong" asp-route-id="@Model.Id"
           class="btn btn-danger">
            Delete
        </a>
    </td>
</tr>
